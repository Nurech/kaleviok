{
  "name": "kaleviok",
  "version": "0.0.181",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "prebuild": "node ./scripts/update-ngsw.js",
    "start:dev": "gcloud config set project kaleviok-dev && ng serve --configuration development --disable-host-check",
    "start:prod": "gcloud config set project kaleviok-9aeb0 && pnpm run env:prod && ng serve --configuration production --disable-host-check",
    "build:dev": "pnpm run prebuild && ng build --configuration development",
    "build:prod": "pnpm run prebuild ng build --configuration production",
    "bump-version": "node ./scripts/bump-version.js",
    "install:generate-store": "cd generate-store && pnpm install && pnpm run build && pnpm link && cd .. && pnpm i --save-dev ./generate-store",
    "install:functions": "cd functions && pnpm install",
    "cleanup:imports": "ng g @angular/core:cleanup-unused-imports",
    "remove:standalone": "ng g @angular/core:standalone",
    "migrate:signal-inputs": "ng g @angular/core:signal-input-migration",
    "migrate:output-migration": "ng g @angular/core:output-migration",
    "migrate:signal-queries-migration": "ng g @angular/core:signal-queries-migration",
    "lint": "eslint --cache --fix \"**/*.{js,ts,html,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,ts,html,json,css,scss,md}\""
  },
  "lint-staged": {
    "src/**/*.{js,ts,html,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "functions/**/*.{js,ts,html,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.1",
    "@angular/cdk": "^19.1.0",
    "@angular/common": "^19.1.1",
    "@angular/compiler": "^19.1.1",
    "@angular/core": "^19.1.1",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.1",
    "@angular/material": "^19.1.0",
    "@angular/material-luxon-adapter": "^19.1.0",
    "@angular/platform-browser": "^19.1.1",
    "@angular/platform-browser-dynamic": "^19.1.1",
    "@angular/pwa": "^19.2.1",
    "@angular/router": "^19.1.1",
    "@angular/service-worker": "^19.1.1",
    "@material/material-color-utilities": "^0.3.0",
    "@ngrx/effects": "^19.0.0",
    "@ngrx/entity": "^19.0.0",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/router-store": "^19.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngrx/store-devtools": "^19.0.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.1",
    "dotenv": "^16.4.7",
    "firebase": "^11.1.0",
    "ngrx-store-localstorage": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.1.2",
    "@angular/compiler-cli": "^19.1.1",
    "@ngrx/schematics": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "generate-store": "file:generate-store",
    "husky": "^9.1.7",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.18.2"
  }
}
