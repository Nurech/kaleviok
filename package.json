{
  "name": "kaleviok",
  "version": "0.0.116",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "prebuild": "node ./scripts/update-ngsw.js",
    "start:dev": "npm run env:dev && ng serve --configuration development",
    "start:prod": "npm run env:prod && ng serve --configuration production",
    "build:dev": "npm run prebuild && npm run env:dev && ng build --configuration development",
    "build:prod": "npm run prebuild && npm run env:prod && ng build --configuration production",
    "env:dev": "cross-env NODE_ENV=dev node ./scripts/replace-env.js",
    "env:prod": "cross-env NODE_ENV=prod node ./scripts/replace-env.js",
    "bump-version": "node ./scripts/bump-version.js",
    "install:generate-store": "cd generate-store && npm install && npm run build && npm link && cd .. && npm i --save-dev ./generate-store",
    "install:functions": "cd functions && npm install",
    "postinstall": "npm run install:generate-store && npm run install:functions",
    "cleanup:imports": "ng g @angular/core:cleanup-unused-imports",
    "remove:standalone": "ng g @angular/core:standalone",
    "migrate:signal-inputs": "ng g @angular/core:signal-input-migration",
    "migrate:output-migration": "ng g @angular/core:output-migration",
    "migrate:signal-queries-migration": "ng g @angular/core:signal-queries-migration",
    "lint": "eslint \"**/*.{js,ts,html}\"",
    "format": "prettier --write \"**/*.{js,ts,html,json,css,scss,md}\""
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "npm run format",
      "npm run lint -- --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.1",
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.1",
    "@angular/compiler": "^19.1.1",
    "@angular/core": "^19.1.1",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.1",
    "@angular/material": "^19.1.0",
    "@angular/platform-browser": "^19.1.1",
    "@angular/platform-browser-dynamic": "^19.1.1",
    "@angular/pwa": "^19.1.2",
    "@angular/router": "^19.1.3",
    "@angular/service-worker": "^19.1.1",
    "@material/material-color-utilities": "^0.3.0",
    "@ngrx/effects": "^19.0.0",
    "@ngrx/entity": "^19.0.0",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/router-store": "^19.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngrx/store-devtools": "^19.0.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.1",
    "dotenv": "^16.4.7",
    "firebase": "^11.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.1",
    "@ngrx/schematics": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "generate-store": "file:generate-store",
    "husky": "^9.1.7",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.18.2"
  }
}
