{
  "name": "kaleviok",
  "version": "0.0.193",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "install:functions": "cd functions && pnpm install",
    "build:functions": "cd functions && pnpm run build",
    "build:dev": "ng build --configuration development && pnpm run build:functions",
    "build:prod": "ng build --configuration production && pnpm run build:functions",
    "start:dev": "ng serve --configuration development",
    "start:prod": "ng serve --configuration production",
    "bump-version": "node ./scripts/bump-version.js",
    "install:generate-store": "cd generate-store && pnpm install && pnpm run build && pnpm link && cd .. && pnpm i --save-dev ./generate-store",
    "cleanup:imports": "ng g @angular/core:cleanup-unused-imports",
    "remove:standalone": "ng g @angular/core:standalone",
    "migrate:signal-inputs": "ng g @angular/core:signal-input-migration",
    "migrate:output-migration": "ng g @angular/core:output-migration",
    "migrate:signal-queries-migration": "ng g @angular/core:signal-queries-migration",
    "lint": "eslint --cache --fix \"**/*.{js,ts,html,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,ts,html,json,css,scss,md}\""
  },
  "lint-staged": {
    "src/**/*.{js,ts,html,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "functions/**/*.{js,ts,html,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.2",
    "@angular/cdk": "^19.2.3",
    "@angular/common": "^19.2.2",
    "@angular/compiler": "^19.2.2",
    "@angular/core": "^19.2.2",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.2.2",
    "@angular/material": "^19.2.3",
    "@angular/material-luxon-adapter": "^19.2.3",
    "@angular/platform-browser": "^19.2.2",
    "@angular/platform-browser-dynamic": "^19.2.2",
    "@angular/pwa": "^19.2.3",
    "@angular/router": "^19.2.2",
    "@angular/service-worker": "^19.2.2",
    "@material/material-color-utilities": "^0.3.0",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/entity": "^19.0.1",
    "@ngrx/operators": "^19.0.1",
    "@ngrx/router-store": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "dotenv": "^16.4.7",
    "firebase": "^11.4.0",
    "ngrx-store-localstorage": "^19.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.3",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.2",
    "@ngrx/schematics": "^19.0.1",
    "@types/jasmine": "~5.1.7",
    "angular-eslint": "19.2.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "generate-store": "file:generate-store",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.14",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.26.1"
  }
}
